{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "default": 0
    },
    "compare_at_price": {
      "type": "decimal"
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "quantity": {
      "type": "integer",
      "default": 0
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "sub_child_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-child-category.sub-child-category",
      "inversedBy": "products"
    },
    "isNew": {
      "type": "boolean",
      "default": false
    },
    "details": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "information": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
