{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "compareAtPrice": {
      "type": "decimal",
      "default": 0
    },
    "brand": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "reviews": {
      "type": "integer"
    },
    "slug": {
      "type": "uid"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "products"
    },
    "description": {
      "type": "richtext"
    },
    "quantity": {
      "type": "integer"
    }
  }
}
