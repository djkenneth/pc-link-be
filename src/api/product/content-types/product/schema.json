{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "default": 0
    },
    "compare_at_price": {
      "type": "decimal"
    },
    "rating": {
      "type": "integer",
      "default": 0
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "details": {
      "type": "richtext"
    },
    "information": {
      "type": "richtext"
    },
    "quantity": {
      "type": "integer",
      "default": 0
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    }
  }
}
